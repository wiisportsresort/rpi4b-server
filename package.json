{
  "name": "wiisportsresorts.dev",
  "version": "2.0.0-alpha",
  "description": "website",
  "scripts": {
    "start": "concurrently -i -n \"bld:f,bld:b,server\" -c \"white.bgBlue.bold,white.bgCyan.bold,white.bgGreen.bold\" \"pnpm run bld-f\" \"pnpm run bld-b\" && concurrently -n server -c \"white.bgGreen.bold\" \"pnpm run server-ssl\"",
    "dev": "concurrently -i -n \"bld:f,bld:b,server\" -c \"white.bgBlue.bold,white.bgCyan.bold,white.bgGreen.bold\" \"pnpm run bld-f:watch\" \"pnpm run bld-b:watch\" \"pnpm run nodemon\"",
    "debug": "concurrently -i -n \"bld:f,bld:b,server\" -c \"white.bgBlue.bold,white.bgCyan.bold,white.bgGreen.bold\" \"pnpm run bld-f:watch\" \"pnpm run bld-b:watch\" \"pnpm run nodemon:debug\"",
    "bld-f": "webpack -p --config-name frontend",
    "bld-b": "webpack -p --config-name backend",
    "bld-f:watch": "webpack -d --config-name frontend --watch",
    "bld-b:watch": "webpack -d --config-name backend --watch",
    "server-ssl": "node \"./dist/backend/https.js\"",
    "nodemon": "nodemon \"./dist/backend/http.js\"",
    "nodemon:debug": "nodemon --inspect \"./dist/backend/http.js\""
  },
  "config": {
    "colors": "white.bgBlue.bold,white.bgCyan.bold,white.bgGreen.bold",
    "names": "bld:f,bld:b,server",
    "built": {
      "https": "./dist/backend/https.js",
      "http": "./dist/backend/http.js"
    }
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "dist"
    ]
  },
  "license": "MIT",
  "dependencies": {
    "@blueprintjs/core": "^3.24.0",
    "@blueprintjs/icons": "^3.14.0",
    "express": "^4.17.1",
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.3",
    "@types/node": "^13.9.5",
    "@types/react": "^16.9.29",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "autoprefixer": "^9.7.5",
    "concurrently": "^5.1.0",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "file-loader": "^6.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "terser-webpack-plugin": "^2.3.5",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  }
}
